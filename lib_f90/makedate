#!/bin/sh
#
# This script creates 'date.inc' to include the compiling date
# in the program startup logo.
#

echo "!     date.inc"
echo "!     Created by makedate .."
echo "! "
echo "      character(35)  cdate"
echo "      parameter      (cdate='`date`')"
echo "      character(10)  aktuell"
echo "      parameter      (aktuell='$1')"
